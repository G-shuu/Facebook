<div class="container">
  <div class="row">
    <div class="wrapper col-md-6 col-sm-6 col-md-offset-3 col-sm-offset-3">
      <h2>ユーザー一覧</h2>
      <% @users.each do |user| %>
      <ul>
        <li><%= profile_img(user) %></li>
        <li><%= user.name %></li>
        <li><%= render 'follow_form', user: user %></li>
      </ul>
      <hr>
    </div>
  </div>
      <% end %>
</div>
